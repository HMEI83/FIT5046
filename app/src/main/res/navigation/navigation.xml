<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/eventsFragment">

    <fragment
        android:id="@+id/eventsFragment"
        android:name="com.example.fit5046_ass3.EventsFragment"
        android:label="All Events "
        tools:layout="@layout/fragment_events">
        <action
            android:id="@+id/action_eventsFragment_to_addEventFragment"
            app:destination="@id/addEventFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/addEventFragment"
        android:name="com.example.fit5046_ass3.AddEventFragment"
        android:label="Create Event"
        tools:layout="@layout/fragment_add_event" />
    <fragment
        android:id="@+id/myBookingFragment"
        android:name="com.example.fit5046_ass3.MyBookingFragment"
        android:label="My Booking"
        tools:layout="@layout/my_booking_fragment" />
    <fragment
        android:id="@+id/myEventFragment"
        android:name="com.example.fit5046_ass3.MyEventFragment"
        android:label="My Event"
        tools:layout="@layout/my_event_fragment" />
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.example.fit5046_ass3.MyProfileFragment"
        android:label="My Profile"
        tools:layout="@layout/my_profile_fragment" />
</navigation>